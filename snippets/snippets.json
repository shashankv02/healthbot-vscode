{
	"New Rule": {
		"scope": "",
		"prefix": "healthbot",
		"body": [
			"healthbot {",
			"\ttopic ${1:topic-name} {",
			"\t\trule ${2:rule-name} {",
            "\t\t\tkeys [ ${3:keys} ];",
            "\t\t\tsynopsis \"${4}\";",
            "\t\t\tdescription \"${5}\";",

            "\t\t\tsensor ${6:sensor-name} {",
            "\t\t\t\t${7:sensor}",
            "\t\t\t}",

            "\t\t\t${8:field}",
            "\t\t\t${9:trigger}",
            "\t\t\t${10:variable}",
            "\t\t\t${11:rule-properties}",

            "\t\t\t}",
            "\t\t}",
            "\t}",
            "}"

		]
    },

    "New iAgent Sensor": {
        "scope": "",
        "prefix": ["iAgent", "sensor"],
        "description": "iAgent sensor",
        "body": [
            "iAgent {",
            "\tfile ${1:file-name};",
            "\ttable ${2:table-name};",
            "\tfrequency ${3:60s};",
            "}"
        ]
    },
