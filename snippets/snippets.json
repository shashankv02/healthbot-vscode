    "New Term": {
        "scope": "",
        "prefix": "term",
        "description": "New term",
        "body": [
            "term ${1:term-name} {",
            "\twhen {",
            "\t\t${2:matches-with} ${3};",
            "}",
            "\tthen {",
            "\t\tstatus {",
            "\t\t\tcolor ${4:red};",
            "\t\t\tmessage \"${5}\";",
            "\t\t}",
            "\t}",
            "}"
        ]
    },
